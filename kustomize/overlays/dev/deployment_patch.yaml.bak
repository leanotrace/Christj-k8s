apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx-2
spec:
  selector:
    matchLabels:
      app: nginx-2
  template:
    metadata:
      labels:
        app: nginx-2
    spec:
      serviceAccountName: anyuid-sa
      containers:
      - image: docker.io/library/nginx
        name: nginx-2
        ports:
        - containerPort: 80


